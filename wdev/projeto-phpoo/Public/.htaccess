option -indexes
RewriteEngine On

# Habilitar O PHP 5.5

AddHandler application/x-httpd-php55 .php

<IfModule mod.security.c>
    SecFilterEngine Off
    SecFilterScanPOST Off
</IfModule>

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^(.\*)$ index.php?p=$1 [QSA]
